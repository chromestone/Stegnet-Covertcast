<div class="tab-content"> <script>
            document.addEventListener("DOMContentLoaded", function(event) {
                $(".score").click(function () {
                    if ($(this).hasClass('active')){
                        return false;
                    }
                    $('.radio').removeClass('active');
                    $(this).addClass('active');

                    $('.tab-content-0 ul').addClass('by-score');
                    $('.tab-content-0 ul').append(function () {
                        return $('.tab-content-0 li').sort(function (a, b) {
                            return $(b).attr("data-score") - $(a).attr("data-score");
                        });
                    });
                    $('.accordeon-content-0 ul').addClass('by-score');
                    $('.accordeon-content-0 ul').append(function () {
                        return $('.accordeon-content-0 li').sort(function (a, b) {
                            return $(b).attr("data-score") - $(a).attr("data-score");
                        });
                    });
                });
                $(".alphabetically").click(function () {
                    if ($(this).hasClass('active')){
                        return false;
                    }
                    $('.radio').removeClass('active');
                    $(this).addClass('active');

                    $('.tab-content-0 ul').removeClass('by-score');
                    $('.accordeon-content-0 ul').removeClass('by-score');
                    $('.tab-content-0 li').sort(function(a,b) {
                        return $(a).data('title').localeCompare($(b).data('title'));
                    }).appendTo('.tab-content-0 ul');
                    $('.accordeon-content-0 li').sort(function(a,b) {
                        return $(a).data('title').localeCompare($(b).data('title'));
                    }).appendTo('.accordeon-content-0 ul');
                });
                $(".brand-filter").on("keyup", function() {
                    var value = $(this).val().toLowerCase();
                    $(".tab-content-0 li").filter(function() {
                        $(this).toggle($(this).find('a').text().toLowerCase().indexOf(value) > -1)
                    });
                    $(".accordeon-content-0 li").filter(function() {
                        $(this).toggle($(this).find('a').text().toLowerCase().indexOf(value) > -1)
                    });
                });
            });
        </script>
