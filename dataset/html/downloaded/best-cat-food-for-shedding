                    var chart = new google.visualization.PieChart(document.getElementById('cliverse-chart-25'));

                    chart.draw(data, options, loader());

                    function loader(){
                        jQuery('.cliverse-chart .loader').css('display','none');
                    }

                    jQuery('#cliverse-chart-25 g path').each(function (index){
                        var color = jQuery(this).attr('fill');
                        var val = jQuery(this).next().text();

                        jQuery('.cliverse-chart.cliverse-chart-25 .chart-labels .label').each(function () {
                            labelVal = jQuery(this).attr('value')

                            if (val == labelVal){
                                jQuery(this).find('span.color').css('background', color);
                            }
                        });
                    });
                }
            });
