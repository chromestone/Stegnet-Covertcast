            document.addEventListener("DOMContentLoaded", function(event) {
                $ = jQuery;
                $(document).on('click', '.sourses-block .sourses-title', function () {
                    var title = $(this),
                        info = title.next();
                    if (!title.hasClass('active')) {
                        $('.sourses-block .sourses-title').removeClass('active');
                        title.addClass('active');
                        $(info).slideUp(400);
                        info.slideDown(400);
                    } else {
                        $('.sourses-block .sourses-title').removeClass('active').next().slideUp(400);
                    }
                    return false;
                });
            });
