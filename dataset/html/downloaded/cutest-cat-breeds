 var dataRelParts = dataRel.split(' '); var newRelParts = arrayUnique( relParts.concat(dataRelParts) ); var newRel = newRelParts.join(' '); link.attr('rel', newRel); } else { link.attr('rel', dataRel); } link.removeAttr('data-rel'); } }); }, 1000); }); jQuery('#ajaxsearchlite1 .promagnifier').click(function() { jQuery('#ajaxsearchlite1 form').submit(); }); var is_h1 = 0; /*console.log(gaData);*/ if (typeof gaData != 'undefined') { for (var i in gaData) { var experiments = gaData[i].experiments; for (var j in experiments) { is_h1 = parseInt(experiments[j]); break; } break; } } var productBlocks = jQuery('.cliverse-product-block'); if (productBlocks.length) { var id = 0; productBlocks.each(function() { id++; var block = jQuery(this); block.attr('data-id', id); }); jQuery('.cliverse-product-block a').click(function() { var link = jQuery(this); var block = link.parents('.cliverse-product-block'); var data = { page_id: '45741', id: block.attr('data-id'), action: 'cliverse_picked_count' }; var ajax_url = 'https://allaboutcats.com/wp-admin/admin-ajax.php'; jQuery.post(ajax_url, data, function(response) { console.log(response); }); }); } }); </script> <script> window.addEventListener('firstUserActivity', function (e) { var script = document.createElement('script'); script.src = '//static.klaviyo.com/onsite/js/klaviyo.js?company_id=PhqJZC'; script.async = true; document.head.appendChild(script); }); </script> <style> #cookie-law-info-bar { padding: 14px 25px; background-color: rgb(255, 255, 255); color: rgb(51, 51, 51); font-family: inherit; bottom: 0px; position: fixed; display: block; z-index: 1000000!important; font-size: 15px; margin: 0 auto; text-align: center; box-sizing: border-box; width: 100%; left: 0; font-weight: 300; box-shadow: 0 -1px 10px 0 rgb(172 171 171 / 30%); } .cli-bar-container { float: none; margin: 0 auto; display: -webkit-box; display: -moz-box; display: -ms-flexbox; display: -webkit-flex; display: flex; justify-content: space-between; -webkit-box-ali