<script>
        document.addEventListener("DOMContentLoaded", function(event) {
            $ = jQuery;

            var index = 0;

            $(".tabs .tab-title").each(function (){
                var currentTitle = $(this);
                var currentAccordeonContent = $('<div class="accordeon-content accordeon-content-' + index + '" style="display: none"></div>');
                var currentTabContent = $(currentTitle).parents('.tabs-block').find('.tab-content').eq(index);

                currentTitle.attr('data-index', index);
                currentTabContent.addClass('tab-content-' + index);
                currentAccordeonContent.insertAfter(currentTitle).html(currentTabContent.html());

                index++;
            });

            $(".tabs .tab-title").click(function () {
                var currentTitle = $(this);
                var index = currentTitle.attr('data-index');

                if ($(window).width() < 769){
                    if (currentTitle.hasClass('active-accordion')) {
                        $('.tabs .tab-title.active-accordion').removeClass('active-accordion');
                        $('.accordeon-content-' + index).slideUp(300);
                    } else {
                        currentTitle.addClass("active-accordion");
                        $('.accordeon-content-' + index).slideDown(300);
                    }
                }
                if ($(window).width() > 768){
                    if (!currentTitle.hasClass('active-tab')) {
                        $('.tabs .tab-title.active-tab').removeClass('active-tab');

                        currentTitle.addClass("active-tab");

                        $('.tab-content').hide(0);
                        $('.tab-content-' + index).slideDown(300);
                    }
                }

            });

            $(".tabs .tab-title:first").click();

            $(".tabs .tab-title").each(function (index){
                var currentTab = $('.tabs-block .tab-content').eq(index);
                var currentTabElementsQuantity = currentTab.find('li').length;
                var currentTabText = $(this).text();

                $(this).text(currentTabText + '  (' + currentTabElementsQuantity + ')');
            });

        });
    </script>
