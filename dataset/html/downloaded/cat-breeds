<script>
                    document.addEventListener("DOMContentLoaded", function(event) {
                        var apiUrl = 'https://allaboutcats.com/api/catbreeds/';

                        var search = jQuery('.breed-search');

                        var waiting = false;

                        var scrollToItems = function () {
                            var items = jQuery('.breeds-items').first();

                            jQuery('html, body').animate({scrollTop:(items.offset().top - 50)}, 1000);
                        };

                        var updateBreedsCount = function() {
                            var breedItemsLength = jQuery('.breeds-items .breeds-item').length;

                            jQuery('.breeds-items-title .number').html(breedItemsLength);
                        }

                        var updateBreeds = function(params) {
                            if (!waiting) {
                                waiting = true;

                                setTimeout(function() {
                                    jQuery.get(apiUrl, params, function(response) {
                                        jQuery('.breeds-items').html(response);
                                        
                                        updateBreedsCount();
                                    });

                                    waiting = false;
                                }, 1);
                            }
                        };

                        var updateBottomFilterBreeds = function() {
                            var playfulness = jQuery('#slidervalueplay').val();
                            var activity_level = jQuery('#slidervalueactivity').val();

                            var catbreed_size = jQuery('.breeds-filter-size-checkbox.active');

                            if (catbreed_size.length) {
                                catbreed_size = catbreed_size.text();
                            } else {
                                catbreed_size = '';
                            }

                            var catbreed_coat_type = jQuery('.breeds-filter-coat-checkbox.active');

                            if (catbreed_coat_type.length) {
                                catbreed_coat_type = catbreed_coat_type.text();
                            } else {
                                catbreed_coat_type = '';
                            }

                            var params = {
                                playfulness: playfulness,
                                activity_level: activity_level,
                                catbreed_size: catbreed_size,
                                catbreed_coat_type: catbreed_coat_type,
                            };

                            updateBreeds(params);
                        };

                        search.change(function() {
                            jQuery('.breeds-carousel .item-panel.active').removeClass('active');
                            jQuery('.breeds-letter-button.active').removeClass('active');
                            jQuery('.breeds-filter-checkbox.active').removeClass('active');
                            sliderplay.noUiSlider.set([0, 100]);
                            slideractivity.noUiSlider.set([0, 100]);

                            var params = {
                                search: search.val()
                            };

                            updateBreeds(params);

                            scrollToItems();
                        });

                        jQuery(document.body).on('click', '.breeds-loadmore-button', function() {
                            var button = jQuery(this);

                            var params = {
                                search: button.attr('data-search'),
                                catbreed_category: button.attr('data-catbreed_category'),
                                catbreed_size: button.attr('data-catbreed_size'),
                                catbreed_coat_type: button.attr('data-catbreed_coat_type'),
                                activity_level: button.attr('data-activity_level'),
                                playfulness: button.attr('data-playfulness'),
                                numberposts: button.attr('data-numberposts'),
                                page: button.attr('data-page'),
                            };

                            jQuery('.breeds-loadmore').remove();

                            jQuery.get(apiUrl, params, function(response) {
                                jQuery('.breeds-items').append(response);

                                updateBreedsCount();
                            });

                            return false;
                        });

                        var sliderplay = document.getElementById('sliderplay');

                        noUiSlider.create(sliderplay, {
                            start: [0, 100],
                            connect: true,
                            step: 10,
                            range: {
                                'min': 0,
                                'max': 100
                            }
                        });

                        var slidervalueplay = document.getElementById('slidervalueplay');

                        sliderplay.noUiSlider.on('update', function (values, handle) {
                            var value1 = Math.round(values[0]);
                            var value2 = Math.round(values[1]);

                            slidervalueplay.value = value1 + '-' + value2;

                            if (jQuery('.breeds-filter:hover').length) {
                                search.val('');
                                jQuery('.breeds-carousel .item-panel.active').removeClass('active');
                                jQuery('.breeds-letter-button.active').removeClass('active');

                                updateBottomFilterBreeds();
                            }
                        });

                        var slideractivity = document.getElementById('slideractivity');

                        noUiSlider.create(slideractivity, {
                            start: [0, 100],
                            connect: true,
                            step: 10,
                            range: {
                                'min': 0,
                                'max': 100
                            }
                        });

                        var slidervalueactivity = document.getElementById('slidervalueactivity');

                        slideractivity.noUiSlider.on('update', function (values, handle) {
                            var value1 = Math.round(values[0]);
                            var value2 = Math.round(values[1]);

                            slidervalueactivity.value = value1 + '-' + value2;

                            if (jQuery('.breeds-filter:hover').length) {
                                search.val('');
                                jQuery('.breeds-carousel .item-panel.active').removeClass('active');
                                jQuery('.breeds-letter-button.active').removeClass('active');

                                updateBottomFilterBreeds();
                            }
                        });

                        jQuery(document).on('click', '.breeds-carousel .item-panel', function() {
                            search.val('');
                            sliderplay.noUiSlider.set([0, 100]);
                            slideractivity.noUiSlider.set([0, 100]);
                            jQuery('.breeds-letter-button.active').removeClass('active');
                            jQuery('.breeds-filter-checkbox.active').removeClass('active');

                            var item = jQuery(this);

                            if (item.hasClass('active')) {
                                item.removeClass('active');

                                var params = {
                                    catbreed_category: ''
                                };

                                updateBreeds(params);
                            } else {
                                jQuery('.breeds-carousel .item-panel').removeClass('active');

                                item.addClass('active');

                                var params = {
                                    catbreed_category: item.find('.item-title').text()
                                };

                                updateBreeds(params);

                                scrollToItems();
                            }

                            return false;
                        });

                        jQuery('.breeds-letter-button').click(function() {
                            search.val('');
                            sliderplay.noUiSlider.set([0, 100]);
                            slideractivity.noUiSlider.set([0, 100]);
                            jQuery('.breeds-carousel .item-panel.active').removeClass('active');
                            jQuery('.breeds-filter-checkbox.active').removeClass('active');

                            var item = jQuery(this);

                            if (item.hasClass('active')) {
                                item.removeClass('active');

                                var params = {
                                    search: ''
                                };

                                updateBreeds(params);
                            } else {
                                jQuery('.breeds-letter-button.active').removeClass('active');

                                item.addClass('active');

                                var params = {
                                    search: item.text()
                                };

                                updateBreeds(params);

                                scrollToItems();
                            }
                        });

                        jQuery('.breeds-filter-checkbox').click(function() {
                            search.val('');
                            jQuery('.breeds-carousel .item-panel.active').removeClass('active');
                            jQuery('.breeds-letter-button.active').removeClass('active');
                        });

                        jQuery('.breeds-filter-size-checkbox').click(function() {
                            var item = jQuery(this);

                            if (item.hasClass('active')) {
                                item.removeClass('active');
                            } else {
                                jQuery('.breeds-filter-size-checkbox.active').removeClass('active');

                                item.addClass('active');
                            }

                            updateBottomFilterBreeds();
                        });

                        jQuery('.breeds-filter-coat-checkbox').click(function() {
                            var item = jQuery(this);

                            if (item.hasClass('active')) {
                                item.removeClass('active');
                            } else {
                                jQuery('.breeds-filter-coat-checkbox.active').removeClass('active');

                                item.addClass('active');
                            }

                            updateBottomFilterBreeds();
                        });

                        jQuery('.breeds-filter-less-button').click(function() {
                            var item = jQuery('.breeds-filter-panel');

                            if (item.hasClass('active')) {
                                jQuery('.breeds-filter').slideDown(300);
                                item.removeClass('active');
                            } else {
                                jQuery('.breeds-filter').slideUp(300);
                                item.addClass('active');
                            }

                            return false;
                        });

                        jQuery('.owl-carousel').owlCarousel({
                            loop: true,
                            margin: 30,
                            dots: false,
                            nav: true,
                            responsive: {
                                0: {
                                    nav: false,
                                    margin: 15,
                                    items:3
                                },
                                768: {
                                    items:3
                                },
                                1000: {
                                    items:4
                                }
                            }
                        });

                        var compareWrap = jQuery('.compare-wrap');
                        var compareBreeds = jQuery('.compare-wrap .compare-breeds');

                        var updateCompareCount = function() {
                            var items = jQuery('.compare-wrap .compare-breed');
                            var count = items.length;

                            var ids = [];

                            items.each(function() {
                                ids.push(jQuery(this).find('.compare-breed-close').attr('data-id'));
                            });

                            var url = 'https://allaboutcats.com/compare-cat-breeds?ids=' + ids.join('-');

                            jQuery('.compare-button').attr('href', url);

                            jQuery('.compare-count').html(count);

                            if (count) {
                                compareWrap.addClass('active');
                            } else {
                                compareWrap.removeClass('active');
                            }
                        };

                        jQuery(document).on('click', '.breeds-item-compare', function () {
                            var button = jQuery(this);
                            var id = button.attr('data-id');

                            if (button.hasClass('active')) {
                                jQuery('.compare-breed-close[data-id="' + id + '"]').click();
                            } else {
                                if (jQuery('.compare-wrap .compare-breed').length >= 3) {
                                    alert('You can select up to 3 breeds to see a side-by-side comparison.');
                                } else {
                                    compareBreeds.append('<div class="compare-breed"><a href="#" class="compare-breed-close" data-id="'+ button.attr('data-id') +'"></a> ' + button.attr('data-title') + '</div>');

                                    updateCompareCount();

                                    button.addClass('active');
                                }
                            }

                            return false;
                        });

                        jQuery(document).on('click', '.compare-breed-close', function() {
                            var button = jQuery(this);
                            var id = button.attr('data-id');

                            jQuery('.breeds-item-compare[data-id="' + id + '"]').removeClass('active');

                            button.parents('.compare-breed').remove();

                            updateCompareCount();

                            return false;
                        });
                    });
                </script>
