cookie = cname + "=" + cvalue + ";" + expires + ";path=/"; }; var getCookie = function(cname) { var name = cname + "="; var ca = document.cookie.split(';'); for(var i = 0; i < ca.length; i++) { var c = ca[i]; while (c.charAt(0) == ' ') { c = c.substring(1); } if (c.indexOf(name) == 0) { return c.substring(name.length, c.length); } } return ""; }; if (getCookie('CookieLawInfoConsent')) { jQuery('#cookie-law-info-bar').remove(); } else { jQuery('#cookie_action_close_header').click(function() { jQuery('#cookie-law-info-bar').remove(); setCookie('CookieLawInfoConsent', 'Yes'); }); } }); </script>
