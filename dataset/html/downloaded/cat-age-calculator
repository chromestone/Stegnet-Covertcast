<script>
        document.addEventListener("DOMContentLoaded", function(event) {

            $ = jQuery;

             // var changeUrl = function(urlPath){
             //     window.history.pushState({},"", urlPath);
             // };

            //var currentPageUrl = '//';

            window.onpopstate = function(e){
                if(e.state){
                    $('.cat-age-calculator').removeClass('calculated');
                }
            };
            var step = 1;
            $('.cat-age-calculator .question-' + step).addClass('active');

            var changeStep = function(stepDiff) {
                $('.cat-age-calculator .step-line').removeClass('step-' + step);

                step += stepDiff;

                $('.cat-age-calculator .step-line').addClass('step-' + step);
                $('.cat-age-calculator .step-number').html(step);

                $('.cat-age-calculator .question').removeClass('active');
                $('.cat-age-calculator .question-' + step).addClass('active');


                if (stepDiff < 1) {
                    // if (window.location.href.indexOf('?results') >= 0) {
                    //     changeUrl(currentPageUrl);
                    // }

                    $('.calculator').removeClass('calculated');
                    $('.calculator-bottom-block').addClass('active');
                }

            };

            $('.cat-age-calculator .question .next-step').click(function(){
                if ($(this).prev().val() == '') {
                    $(this).parents('.question-box').addClass('error')
                    return  false;
                }

                changeStep(1);

                return false;
            });

            $('form.cat-age-calculator').submit(function (){

                var apiUrl = 'https://allaboutcats.com/wp-admin/admin-ajax.php';

                var data = {
                    age: $(".cat-age-calculator select[name='cat-age']").val(),
                    place: $(".cat-age-calculator select[name='cat-place']").val(),
                    action: 'cliverse_age_calculator_results',
                };

                $.post(apiUrl, data, function(response) {
                    $('.cat-age-calculator').addClass('calculated');
                    $('.cat-age-calculator .result-block .result-text').text(response);
                    $('.cat-age-calculator .result-block').addClass('active');

                    // changeUrl(currentPageUrl + '?results');

                    step++;
                });
                return false;
            });

            $('form.cat-age-calculator .start-over').click(function (){
                $('.cat-age-calculator').removeClass('calculated');
                $('.cat-age-calculator .result-block').removeClass('active');
                $(".cat-age-calculator select[name='cat-age']").val('');
                $(".cat-age-calculator select[name='cat-place']").val('');
                step = 1;
                $('.cat-age-calculator .step-number').html(step);

                $('.cat-age-calculator .question').removeClass('active');
                $('.cat-age-calculator .question-' + step).addClass('active');
            });

        });

    </script>
<form class="cat-age-calculator" method="post">
