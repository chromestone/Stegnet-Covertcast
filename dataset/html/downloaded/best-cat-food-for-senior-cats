 var dataRelParts = dataRel.split(' '); var newRelParts = arrayUnique( relParts.concat(dataRelParts) ); var newRel = newRelParts.join(' '); link.attr('rel', newRel); } else { link.attr('rel', dataRel); } link.removeAttr('data-rel'); } }); }, 1000); }); jQuery('#ajaxsearchlite1 .promagnifier').click(function() { jQuery('#ajaxsearchlite1 form').submit(); }); var is_h1 = 0; /*console.log(gaData);*/ if (typeof gaData != 'undefined') { for (var i in gaData) { var experiments = gaData[i].experiments; for (var j in experiments) { is_h1 = parseInt(experiments[j]); break; } break; } } var productBlocks = jQuery('.cliverse-product-block'); if (productBlocks.length) { var id = 0; productBlocks.each(function() { id++; var block = jQuery(this); block.attr('data-id', id); }); var picked = jQuery('.cliverse-product-block[data-id="1"] .cliverse-picked-count'); if (picked.length) { var pickedCount = 50; pickedCount *= 3; if (picked.hasClass('x2')) { pickedCount *= 2; } pickedCount += 8; picked.html(pickedCount); } var picked = jQuery('.cliverse-product-block[data-id="2"] .cliverse-picked-count'); if (picked.length) { var pickedCount = 32; pickedCount *= 3; if (picked.hasClass('x2')) { pickedCount *= 2; } pickedCount += 7; picked.html(pickedCount); } var picked = jQuery('.cliverse-product-block[data-id="3"] .cliverse-picked-count'); if (picked.length) { var pickedCount = 31; pickedCount *= 3; if (picked.hasClass('x2')) { pickedCount *= 2; } pickedCount += 6; picked.html(pickedCount); } var picked = jQuery('.cliverse-product-block[data-id="4"] .cliverse-picked-count'); if (picked.length) { var pickedCount = 15; pickedCount *= 3; if (picked.hasClass('x2')) { pickedCount *= 2; } pickedCount += 5; picked.html(pickedCount); } var picked = jQuery('.cliverse-product-block[data-id="5"] .cliverse-picked-count'); if (picked.length) { var pickedCount = 14; pickedCount *= 3; if (picked.hasClass('x2')) { pickedCount *= 2; } pickedCount += 4; picked.html(pickedCount); } var picked = jQuery('.clive