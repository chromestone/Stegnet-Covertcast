<script>
    document.addEventListener("DOMContentLoaded", function(event) {
        $ = jQuery;

        $(document).on('change', "select[name='cat-symptom']", function (){
            $(".symptoms-tool-block form button").removeClass('disabled');
            $(".symptoms-tool-block form input[name='form-result']").val($(this).val());
        });

        var startContent = $('.symptoms-tool-block .form-content').html();
        var prevContents = [];

        $('.symptoms-tool-block select').select2();

        $(document).on('submit', 'form.symptoms-tool',function (){
            if (prevContents.length) {
                var prevContent = $('.symptoms-tool-block .form-content').html();

                prevContents.push(prevContent);
            } else {
                prevContents.push(startContent);
            }

            var apiUrl = 'https://allaboutcats.com/wp-admin/admin-ajax.php';
            var value = $("input[name='form-result']").val();

            if(value==''){
                alert('Please choose answer!')
                return false;
            }

            var data = {
                value: value,
                action: 'cliverse_cat_symptom_tool',
            };
            console.log(data);

            $.post(apiUrl, data, function(response) {
                $('form.symptoms-tool .form-content').html(response);
            });
            return false;
        });
        $(document).on('click', '.buttons .answer', function (){
            if(!$(this).hasClass('active')){
                $('.buttons .answer').removeClass('active');
                $(this).addClass('active');
                $(".symptoms-tool-block form input[name='form-result']").val($(this).attr('data-value'));
            }else{
                $(this).removeClass('active');
                $(".symptoms-tool-block form input[name='form-result']").val('');
            }
        });
        $(document).on('click', '.process-buttons .back', function (){
            if (!prevContents.length) {
                return;
            }

            var prevContent = prevContents[prevContents.length - 1];

            prevContents.pop()

            $('form.symptoms-tool .form-content').html(prevContent);
            $('.symptoms-tool-block select').select2();
        });
        $(document).on('click', '.process-buttons .refresh', function (){
            /*$(".symptoms-tool-block form input[name='form-result']").val('');
            $('form.symptoms-tool .form-content').html(startContent);
            $('.symptoms-tool-block select').select2();*/
            window.location.reload();
        });

    });
</script>
